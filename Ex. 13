
def fibonaccibro():
    nummer = int(input("How many fibonacci numbers do you want? "))
    i = 1
    if nummer == 0:
        fib = []
    elif nummer == 1:
        fib = [1]
    elif nummer == 2:
        fib = [1,1]
    elif nummer > 2:
        fib = [1,1]
        while i<(nummer-1):
            fib.append(fib[i]+fib[i-1])
            i += 1
    return fib

print(fibonaccibro())
